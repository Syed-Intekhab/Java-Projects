// Program to calculate average of array elements.

import java.util.*;

public class Main{
    public static void main(String args[]){
        int arr[] = {1, 2, 3, 4, 5};
        int average = 0, sum = 0;
        
        for(int i = 0; i < arr.length; i++){
            sum += arr[i];
        }
        
        average = sum / arr.length;
        
        System.out.println("Average: " + average);
    }
}
